// ConsoleApplication22.cpp: определяет точку входа для консольного приложения.
//

#include "stdafx.h"
#include <iostream>
#include <ctime>			 //random
#include <set>				 //container

using namespace std;
//Local constants
const int row = 3, col = 3; //Size of matrix

int main()
{   
	// To create random generation of matrix
	srand((unsigned)time(0));

	int a[row][col];
	//Loop from matrix size[row][col] - generate random member of matrix
	for (int i = 0; i<row; i++)
		for (int j = 0; j<col; j++)
			a[i][j] = rand() % 10 ; //coefficient for random shouldn't be very high - because we need matchs in the massive
	//Loop again - show matrix on screen
	for (int i = 0; i<row; i++)
	{
		for (int j = 0; j<col; j++)
			cout << a[i][j] << '\t';
		cout << endl;
	}
	set<int> s((int*)a, (int*)a + col*row); // So set contain ONLY unique key in list we can rewrite massive to set and avoid duplicates
	cout << "\nDifferent member of massive: " << s.size() << endl;
	system("pause");
	return 0;
}
